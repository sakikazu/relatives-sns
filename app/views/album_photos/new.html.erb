<h1>New album_photo</h1>

<% form_for(@album_photo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :album_id %><br />
    <%= f.text_field :album_id %>
  </p>
  <p>
    <%= f.label :filename %><br />
    <%= f.text_field :filename %>
  </p>
  <p>
    <%= f.label :minetype %><br />
    <%= f.text_field :minetype %>
  </p>
  <p>
    <%= f.label :filesize %><br />
    <%= f.text_field :filesize %>
  </p>
  <p>
    <%= f.label :exif_at %><br />
    <%= f.datetime_select :exif_at %>
  </p>
  <p>
    <%= f.label :last_comment_at %><br />
    <%= f.datetime_select :last_comment_at %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', album_photos_path %>